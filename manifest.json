{
    "manifest_version": 2,

    "name": "Live Feeds",
    "description": "Improves Live Video Feeds UX http://www.cbs.com/shows/big_brother/live_feed/stream/",
    "version": "0.0.8",

    "background": {
        "scripts": ["background.js"]
    },

    "page_action": {
        "default_title": "Live Feeds UX",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "content_scripts": [{
        "run_at": "document_start",
        "matches": ["http://www.cbs.com/shows/big_brother/live_feed/stream/*"],
        "css": ["content.css"],
        "js": ["jquery-2.1.4.js", "content.js"]
    },{
        "run_at": "document_start",
        "matches": ["http://www.cbs.com/shows/big_brother/live_feed/chat/*"],
        "all_frames": true,
        "css": ["chat.css"],
        "js": ["jquery-2.1.4.js", "chat.js"]
    }],

    "permissions": [
    ]
}